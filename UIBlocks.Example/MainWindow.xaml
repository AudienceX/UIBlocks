<Window x:Class="UIBlocks.Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UIBlocks.Example"
        mc:Ignorable="d"
        Title="各种控件的例子 Example" Height="800" Width="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal">
            <Button Style="{StaticResource MDCirclebutton_LightPurple}" Width="50" Height="50" Content="L" />
            <Button Style="{StaticResource MDCirclebutton_MidPurple}" Width="50" Height="50" Content="L" />
            <Button Style="{StaticResource MDCirclebutton_DarkPurple}" Width="50" Height="50" Content="L" />
        </StackPanel>
    </Grid>
</Window>
